{
    "name": "unzeroun/sorter",
    "description": "A library to dynamically sort collections / queries",
    "type": "library",
    "require": {
        "symfony/http-foundation": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5",
        "doctrine/orm": "^2.5",
        "symfony/property-access": "^2.8|^3.0|^4.0",
        "twig/twig": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "UnZeroUn\\Sorter\\": "src"
        }
    },
    "suggest": {
        "doctrine/orm": "Needed for use of ORMApplier",
        "symfony/property-access": "Needed for use of ArrayAdapter"
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\UnZeroUn\\Sorter\\": "spec"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "yohang",
            "email": "yohan@giarel.li"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
